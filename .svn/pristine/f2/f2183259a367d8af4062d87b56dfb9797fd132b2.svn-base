// Modelo estático


public class Cheque
{

	private string _numero;

	private object _fechaEmision;

	private object _fechaVencimiento;

	private object _fechaRegistracion;

	private object _banco;

	private decimal _importe;

	private long _titular;

	public Cheque()
	{
		
	}

	public object Banco
	{
		get
		{
			return _banco;
		}
		set
		{
			_banco = value;
		}
	}

	public long Titular
	{
		get
		{
			return _titular;
		}
		set
		{
			_titular = value;
		}
	}

	public decimal Importe
	{
		get
		{
			return _importe;
		}
		set
		{
			_importe = value;
		}
	}

	public Cheque(string Numero, System.DateTime fechaEmision, System.DateTime fechaVencimiento)
	{
		_numero = Numero;
		_fechaEmision = fechaEmision;
		_fechaVencimiento = fechaVencimiento;
		_fechaRegistracion = System.DateTime.Now;
	}

	public string Numero
	{
		get 
		{
			return _numero;
		}
	}
	public object FechaEmision
	{
		get
		{
			return _fechaEmision;
		}
	}
	public object FechaVencimiento
	{
		get
		{
			return _fechaVencimiento;
		}
	}

}// END CLASS DEFINITION Cheque
