			uneCostoPresupuesto.DataBindings.Clear();

			txtNombreServicio.DataBindings.Add("Text", _uiController, "NombreServicio");
			uneDuracionDias.DataBindings.Add("Value", _uiController, "DuracionDiasServicio");
			uneCosto.DataBindings.Add("Value", _uiController, "CostoServicio");
