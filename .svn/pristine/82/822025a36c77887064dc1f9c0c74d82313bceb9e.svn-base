		private void _uiController_ServiciosHasChanged(object sender, EventArgs e)
		{			
			mzCEServicios.FillFromDataSource( _uiController.ServiciosEnBD(), "IdFalla", "Descripcion", 50, "1", "2", "Descripcion");
			if(_uiController.IdServicioSeleccionado != long.MinValue)
			{
				mzCEServicios.Value = _uiController.IdServicioSeleccionado;
			}
			else
			{
				btnModificarServicio.Enabled = false;
				btnBorrarServicio.Enabled = false;
			}

		}

		private void _uiController_TareasHasChanged(object sender, EventArgs e)
		{	
			_processItem = false;
			for(int i = 0; i < chkTareas.Items.Count; i++)
				chkTareas.SetItemChecked(i, false);
			foreach(businessrules.Tarea tarea in _uiController.TareasServicioSeleccionado)
			{
				int index = chkTareas.Items.IndexOf(tarea);
				chkTareas.SetItemChecked(index, true);
			}
			_processItem = true;
			txtNombreServicio.Text = _uiController.NombreServicio;			
			uneDuracionDias.Value = _uiController.DuracionDiasServicio;
			uneCosto.Value = _uiController.CostoServicio;
			uneCostoMaximo.Value = _uiController.CostoMaximoServicio;
			uneCostoPresupuesto.Value = _uiController.CostoPresupuestoServicio;
		}

		private void mzCEServicios_SelectionChanged(object sender, EventArgs e)
		{
			_uiController.IdServicioSeleccionado = (long)mzCEServicios.Value;
			btnModificarServicio.Enabled = true;
			//btnBorrarServicio.Enabled = true;
			this.gridTareasServicio.SetDataBinding( null, null );
			this.gridTareasServicio.SetDataBinding(_uiController.TareasServicioSeleccionado, null );
		}

