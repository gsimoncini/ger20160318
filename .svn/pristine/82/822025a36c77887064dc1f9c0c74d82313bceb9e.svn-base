				btnBorrarServicio.Enabled = false;
			}
			panelBotonesABMServicios.Visible = true;
			panelBotonesConfirmacionSevicio.Visible = false;
			mzCEServicios.Enabled = true;
			panelBotonesABMServicios.Enabled = true;
			panelABMServicios.Enabled = false;
			chkTareas.Enabled = false;
		}


		private void chkTareas_ItemCheck(object sender, ItemCheckEventArgs e)
		{
			if(_processItem)
			{
				int index = e.Index;
				bool seleccionada = e.NewValue.Equals(System.Windows.Forms.CheckState.Checked);
				businessrules.Tarea tareaModif = (businessrules.Tarea)chkTareas.Items[index];
				_uiController.ModificarServicioTarea(tareaModif, seleccionada);
			}

		}

