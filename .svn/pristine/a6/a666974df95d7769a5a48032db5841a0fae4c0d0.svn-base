		#region Metodos Privados
		private void SaveChanges()
		{	
			Util.MoveFocus(this.ultraExplorerBarContainerControl2);
			this.Cursor = Cursors.WaitCursor;
			_uiController.Descripcion = txtDescripcion.Text;		
			_uiController.Codigo = txtCodigo.Text;			
			_uiController.IdProducto = mzProductosControl.GetIdProducto();
			_uiController.Complejidad = cboComplejidad.Text;			
			_uiController.Duracion = Convert.ToDecimal(uneDuracion.Value);
			_uiController.ValorTecnico = Convert.ToDecimal(uneValorTecnico.Value);
			_uiController.ValorCliente = Convert.ToDecimal(uneValorCliente.Value);
			_uiController.ValorTecnicoProvisorio = Convert.ToDecimal(uneValorTecnicoProvisorio.Value);
			if(_uiController.Descripcion.Equals(string.Empty))
				MessageBox.Show("Debe ingresar un nombre para la tarea","Advertencia",System.Windows.Forms.MessageBoxButtons.OK);
			else if(_uiController.ExisteNombreTarea()) 
				MessageBox.Show("El nombre de la tarea ingresada ya existe, por favor elija otro","Advertencia",System.Windows.Forms.MessageBoxButtons.OK);
			else
			{
				_uiController.SaveChanges();		
				btnAgregarTarea.Enabled = true;
				if(!_uiController.Descripcion.Equals(string.Empty))
				{
					btnModificarTarea.Enabled = true;
					//btnBorrarTarea.Enabled = true;
				}
				else
				{
					btnModificarTarea.Enabled = false;
					btnBorrarTarea.Enabled = false;
				}
				panelBotonesABMTareas.Visible = true;
				panelBotonesConfirmacionTarea.Visible = false;
				mzCETareas.Enabled = true;
				panelBotonesABMTareas.Enabled = true;
				panelABMTareas.Enabled = false;
				btnAceptar.Enabled = false;
				btnCancelar.Enabled = false;
			}
			this.Cursor = Cursors.Default;

		}

		#endregion

		private void toolBarStandar_ButtonClick(object sender, ToolBarButtonClickEventArgs e)
		{
			switch ( toolBarStandar.Buttons.IndexOf( e.Button ) )
			{
				case 7:
					this.Close();
					break;
			}				
		}

		private void _uiController_TareaSeleccionadaHasChanged(object sender, EventArgs e)
		{	
			txtDescripcion.Text = _uiController.Descripcion;		
			txtCodigo.Text = _uiController.Codigo;			
			mzProductosControl.SetIdProducto(_uiController.IdProducto);
			cboComplejidad.Text = _uiController.Complejidad;			
			uneDuracion.Value = _uiController.Duracion;
			uneValorTecnico.Value = _uiController.ValorTecnico;
			uneValorCliente.Value = _uiController.ValorCliente;
			uneValorTecnicoProvisorio.Value = _uiController.ValorTecnicoProvisorio;
		}

		private void _uiController_TareasHasChanged(object sender, EventArgs e)
		{	
			mzCETareas.FillFromDataSource( _uiController.TareasEnBD(), "IdTarea", "Descripcion", 50, "1", "2", "Descripcion");
			if(_uiController.IdTareaSeleccionada != long.MinValue)
			{
				mzCETareas.Value = _uiController.IdTareaSeleccionada;
			}
			else
			{
				btnModificarTarea.Enabled = false;
				btnBorrarTarea.Enabled = false;
			}
		}

		private void mzCETareas_SelectionChanged(object sender, EventArgs e)
		{
			_uiController.IdTareaSeleccionada = (long)mzCETareas.Value;
			btnModificarTarea.Enabled = true;

		}

		private void btnAgregarTarea_Click(object sender, EventArgs e)
		{
			panelABMTareas.Enabled = true;
			btnAceptar.Enabled = true;
			btnCancelar.Enabled = true;
			btnAgregarTarea.Enabled = false;
			btnModificarTarea.Enabled = false;
			btnBorrarTarea.Enabled = false;
			_uiController.AddTarea();
			panelBotonesABMTareas.Visible = false;
			panelBotonesConfirmacionTarea.Visible = true;
			mzCETareas.Enabled = false;

		}

		private void btnModificarTarea_Click(object sender, EventArgs e)
		{
			panelABMTareas.Enabled = true;		
			btnAceptar.Enabled = true;
			btnCancelar.Enabled = true;
			btnAgregarTarea.Enabled = false;
			btnModificarTarea.Enabled = false;
			btnBorrarTarea.Enabled = false;
			_uiController.ModificarTarea();
			panelBotonesABMTareas.Visible = false;
			panelBotonesConfirmacionTarea.Visible = true;
			mzCETareas.Enabled = false;

		}

		private void btnCancelar_Click(object sender, EventArgs e)
		{
			this._uiController_TareasHasChanged(sender, new EventArgs());
			btnAceptar.Enabled = false;
			btnCancelar.Enabled = false;
			btnAgregarTarea.Enabled = true;
			if(!_uiController.Descripcion.Equals(string.Empty))
			{
				btnModificarTarea.Enabled = true;
				//btnBorrarTarea.Enabled = true;
			}
			else
			{
				btnModificarTarea.Enabled = false;
				btnBorrarTarea.Enabled = false;
			}
			panelBotonesABMTareas.Visible = true;
			panelBotonesConfirmacionTarea.Visible = false;
			mzCETareas.Enabled = true;
			panelBotonesABMTareas.Enabled = true;
			panelABMTareas.Enabled = false;

		}

		private void btnAceptar_Click(object sender, EventArgs e)
		{
			SaveChanges();

		}
	}
