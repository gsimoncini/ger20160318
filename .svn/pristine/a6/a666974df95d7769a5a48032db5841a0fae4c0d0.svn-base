				btnModificarTarea.Click -= new EventHandler(btnModificarTarea_Click);
				btnCancelar.Click -= new EventHandler(btnCancelar_Click);
				btnAceptar.Click -= new EventHandler(btnAceptar_Click);
			}
			base.Dispose( disposing );
		}
