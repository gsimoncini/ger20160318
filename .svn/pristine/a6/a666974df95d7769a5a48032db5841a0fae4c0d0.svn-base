		#region Inicializadores y Carga
		

		protected void Build_UI()
		{
			ConfigureInterface();	
			InitializeData();
			InitEventHandlers();
			InitDataBindings();		
			mzCETareas.FillFromDataSource( _uiController.TareasEnBD(), "IdTarea", "Descripcion", 50, "1", "2", "Descripcion");
			if(_uiController.IdTareaSeleccionada != long.MinValue)
			{
				mzCETareas.Value = _uiController.IdTareaSeleccionada;
			}
			else
			{
				btnModificarTarea.Enabled = false;
				btnBorrarTarea.Enabled = false;
				panelABMTareas.Enabled = false;
			}
		}

		
		private void ConfigureInterface()
		{

			this.Text = _uiController.LeyendaFormulario;
			labelTarea.Text = _uiController.LeyendaFormulario;	

			mzProductosControl.VisiblePrecios = false;			
			mzProductosControl.EnabledListaDePrecio = false;

			btnAceptar.Enabled = false;
			btnCancelar.Enabled = false;

			panelBotonesABMTareas.Visible = true;
			panelBotonesConfirmacionTarea.Visible = false;
			KeyPreview = true;
		
			string listaComplejidades = _uiController.ListaComplejidades;

			if(listaComplejidades == null || listaComplejidades.Equals(string.Empty))
				cboComplejidad.Items.AddRange(new object [] {"A", "B", "C", "D"});
			else
			{
				ArrayList aux = new ArrayList();
				aux.AddRange(mz.erp.systemframework.Util.Parse(listaComplejidades,","));
				cboComplejidad.Items.AddRange(aux.ToArray());					
			}
		}

