    	rpt.PageSettings.Margins.Top = ActiveReport.CmToInch(2);*/

  	rpt.StyleSheet.Add("MyStyle");
  
  	rpt.StyleSheet["MyStyle"].FontName = "Arial";
   	rpt.StyleSheet["MyStyle"].FontSize =null;
   	rpt.StyleSheet["MyStyle"].FontStrikeThrough = false;
